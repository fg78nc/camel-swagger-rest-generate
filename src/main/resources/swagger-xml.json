{
  "swagger": "2.0",
  "info": {
    "version": "4.0.0",
    "title": "Companies Data API",
    "description": "\"Companies API\" provides detailed reports on companies in the selected regions.",
    "contact": {
      "email": "fg78nc@gmail.com",
      "name": "Integration HUB development team",
      "url": "http://www.example.com"
    }
  },
  "host": "virtserver.swaggerhub.com",
