    "application/json"
  ],
  "paths": {
    "/authenticate": {
      "post": {
        "description": "Authenticates user against credentials and returns authorization token for further interactions with the API",
        "summary": "Authenticates user against credentials and returns authorization token for further interactions with the API",
        "tags": [
          "authenticate"
        ],
        "operationId": "AuthenticatePost",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Content-Type",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Must be set to \\'application/json\\' for proper content negotiation"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "description": "Credentials (username and password) used to authenticate user",
            "schema": {
              "$ref": "#/definitions/AuthenticationRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful authentication, access token will be provided in the response message",
            "schema": {
              "$ref": "#/definitions/AuthenticationSuccessResponse"
            },
            "headers": {}
          },
          "400": {
            "description": "Request could not be understood by the server.",
            "schema": {
              "$ref": "#/definitions/ClientRequestErrorResponse"
            },
            "examples": {
              "application/json": {
                "correlationId": "ddd65e40-0deb-11e9-9ae2-02b5324a36fc",
                "message": "Bad client request",
                "details": "Request could not be understood by the server due to malformed syntax. Please check amd modify the structure of your JSON and repeat your request."
              }
            }
          },
          "401": {
            "description": "Unsuccessful authentication, error message will be provided in the response",
            "schema": {
              "$ref": "#/definitions/AuthenticationErrorResponse"
            }
          },
          "403": {
            "description": "Request is not authorized, error message will be provided in the response",
            "schema": {}
          },
          "404": {
            "description": "No response mapped to the request uri",
            "schema": {}
          },
          "429": {
            "description": "Request rate limit exceeded",
            "schema": {}
          },
          "default": {
            "description": "Server error",
            "schema": {}
          }
        }
      }
    },
    "/companies/search": {
      "get": {
        "description": "Given search parameters, provides matched list of companies.",
        "summary": "Given search parameters, provides matched list of companies.",
        "tags": [
          "companies"
        ],
        "operationId": "CompaniesSearchGet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authentication",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Basic authentication with base64 encoded string - authentication token provided by Authorization endpoint."
          },
          {
            "name": "countries",
            "in": "query",
            "required": true,
            "type": "string",
            "description": "Comma separated list of country codes"
          },
          {
            "name": "id",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Company Id"
          },
          {
            "name": "name",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Company name"
          },
          {
            "name": "safeNo",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "API proprietary no"
          },
          {
            "name": "regNo",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Company registration number"
          },
          {
            "name": "vatNo",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "VAT number"
          },
          {
            "name": "street",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Street address"
          },
          {
            "name": "city",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "City"
          },
          {
            "name": "postCode",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Post code"
          },
          {
            "name": "status",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Company status"
          },
          {
            "name": "phone",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Company phone number"
          },
          {
            "name": "<<queryParamSearch>>",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Arbitrary parameter than can be passed in. Can be county dependent"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "default": 1,
            "type": "integer",
            "format": "int32",
            "exclusiveMaximum": false,
            "exclusiveMinimum": false,
            "description": "Starting position in the list of items"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "default": 10,
            "type": "integer",
            "format": "int32",
            "exclusiveMaximum": false,
            "exclusiveMinimum": false,
            "description": "Maximum number of items to be returned"
          },
          {
            "name": "sortBy",
            "in": "query",
            "required": false,
            "default": "name",
            "type": "string",
            "description": "Comma separated list of parameters, defining sorting criteria"
          },
          {
            "name": "sortOrder",
            "in": "query",
            "required": false,
            "default": "asc",
            "type": "string",
            "description": "Sorting order of retrieved results"
          }
        ],
        "responses": {
          "200": {
            "description": "Success - list of companies matching search parameters will be provided",
            "schema": {
              "$ref": "#/definitions/CompaniesSearchResponse"
            },
            "examples": {
              "application/json": {
                "value": {
                  "id": "GB001-1-4257085",
                  "country": "GB",
                  "safeNo": "UK11538192",
                  "name": "DHL",
                  "address": {
                    "line": "LINK 20 BUSINESS PARK, LARKFIELD, AYLESFORD, KENT, ME20 6SP",
                    "street": "LARKFIELD",
                    "city": "AYLESFORD",
                    "postCode": "ME20 6SP"
                  },
                  "status": "Active",
                  "type": "NonLtd",
                  "dateOfLatestChange": "2011-09-27T23:00:00Z"
                }
              }
            },
            "headers": {}
          },
          "401": {
            "description": "Unsuccessful authentication, error message will be provided in the response",
            "schema": {
              "$ref": "#/definitions/AuthenticationErrorResponse"
            }
          },
          "403": {
            "description": "Request is not authorized, error message will be provided in the response",
            "schema": {}
          },
          "404": {
            "description": "No response mapped to the request uri",
            "schema": {}
          },
          "429": {
            "description": "Request rate limit exceeded",
            "schema": {}
          },
          "default": {
            "description": "Server error",
            "schema": {}
          }
        }
      }
    },
    "/companies/{id}": {
      "get": {
        "description": "Given companyId provides report on the company",
        "summary": "Provides company report",
        "tags": [
          "companies"
        ],
        "operationId": "CompaniesByIdGet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authentication",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Basic authentication with base64 encoded string - authentication token provided by Authorization endpoint."
          },
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "The identifier of the company the report is requested on"
          },
          {
            "name": "language",
            "in": "query",
            "required": false,
            "default": "en",
            "type": "string",
            "pattern": "^[a-zA-Z]{2}$",
            "description": "The language of the requested report"
          },
          {
            "name": "template",
            "in": "query",
            "required": false,
            "default": "full",
            "type": "string",
            "description": "Report template"
          }
        ],
        "responses": {
          "200": {
            "description": "Provides report on the company",
            "schema": {
              "$ref": "#/definitions/CompanyReportResponse"
            },
            "headers": {}
          },
          "401": {
            "description": "Unsuccessful authentication, error message will be provided in the response",
            "schema": {
              "$ref": "#/definitions/AuthenticationErrorResponse"
            }
          },
          "403": {
            "description": "Request is not authorized, error message will be provided in the response",
            "schema": {}
          },
          "404": {
            "description": "No response mapped to the request uri",
            "schema": {}
          },
          "429": {
            "description": "Request rate limit exceeded",
            "schema": {}
          },
          "default": {
            "description": "Server error",
            "schema": {}
          }
        }
      }
    },
    "/companies/search/parameters": {
      "get": {
        "description": "Given list of countries, provides information on valid search parameters to narrow search",
        "summary": "Provides list of search parameters",
        "tags": [
          "companies"
        ],
        "operationId": "CompaniesSearchParametersGet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authentication",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Basic authentication with base64 encoded string - authentication token provided by Authorization endpoint."
          },
          {
            "name": "countries",
            "in": "query",
            "required": true,
            "type": "string",
            "description": "Comma separated list of country codes"
          }
        ],
        "responses": {
          "200": {
            "description": "Success - list of valid search parameters applicable to the selected countries will be provided",
            "schema": {
              "$ref": "#/definitions/SearchParametersResponse"
            },
            "headers": {}
          },
          "400": {
            "description": "Request could not be understood by the server.",
            "schema": {
              "$ref": "#/definitions/ClientRequestErrorResponse"
            },
            "examples": {
              "application/json": {
                "correlationId": "ddd65e40-0deb-11e9-9ae2-02b5324a36fc",
                "message": "Bad client request",
                "details": "Error 436 - specified search parameters combination is not supported. Please modify request and retry."
              }
            }
          },
          "401": {
            "description": "Unsuccessful authentication, error message will be provided in the response",
            "schema": {
              "$ref": "#/definitions/AuthenticationErrorResponse"
            }
          },
          "403": {
            "description": "Request is not authorized, error message will be provided in the response",
            "schema": {}
          },
          "404": {
            "description": "No response mapped to the request uri",
            "schema": {}
          },
          "429": {
            "description": "Request rate limit exceeded",
            "schema": {}
          },
          "default": {
            "description": "Server error",
            "schema": {}
          }
        }
      }
    },
    "/companies/countries": {
      "get": {
        "description": "Provides list of countries available for company reports",
        "summary": "Provides list of countries",
        "tags": [
          "companies"
        ],
        "operationId": "CompaniesCountriesGet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authentication",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Basic authentication with base64 encoded string - authentication token provided by Authorization endpoint."
          }
        ],
        "responses": {
          "200": {
            "description": "Success - list of available countries will be provided",
            "schema": {
              "$ref": "#/definitions/CountriesResponse"
            },
            "headers": {}
          },
          "400": {
            "description": "Request could not be understood by the server.",
            "schema": {
              "$ref": "#/definitions/ClientRequestErrorResponse"
            },
            "examples": {
              "application/json": {
                "correlationId": "ddd65e40-0deb-11e9-9ae2-02b5324a36fc",
                "message": "Bad client request",
                "details": "Error 437 - not valid required query parameter `id`. Please modify request and retry."
              }
            }
          },
          "401": {
            "description": "Unsuccessful authentication, error message will be provided in the response",
            "schema": {
              "$ref": "#/definitions/AuthenticationErrorResponse"
            }
          },
          "403": {
            "description": "Request is not authorized, error message will be provided in the response",
            "schema": {}
          },
          "404": {
            "description": "No response mapped to the request uri",
            "schema": {}
          },
          "429": {
            "description": "Request rate limit exceeded",
            "schema": {}
          },
          "default": {
            "description": "Server error",
            "schema": {}
          }
        }
      }
    }
  },
  "definitions": {
    "AuthenticationRequest": {
      "title": "AuthenticationRequest",
      "description": "JSON to be provided in the request body for API authentication",
      "example": {
        "username": "user123",
        "password": "er$yg^#eyd23tg652da"
      },
      "type": "object",
      "properties": {
        "username": {
          "example": "user123",
          "type": "string"
        },
        "password": {
          "example": "er$yg^#eyd23tg652da",
          "type": "string"
        }
      },
      "required": [
        "username",
        "password"
      ]
    },
    "AuthenticationSuccessResponse": {
      "title": "AuthenticationSuccessResponse",
      "description": "Authorization token to be used for subsequent API interactions. Expires in 2 hours.",
      "example": {
        "auth_token": "eyJraWQiOiJNSytSKzRhYUk4YjBxVkhBMkZLZFN4Ykdpb3RXbTNXOGhZWE45dXF3K3YwPSIsImFsZyI6IlJTMjU2In0..KS5SQZcc3BdyIAmAfhrLDDYO7ebNvROEW-Su0nRX2mK12H4529vawoigVkn3Xb653TRFx6fxJe_BiA3yPb0qP0_ZYhNQ_rQAL0zF93E82vBn-nHSjotWcknxqgzuMw-hMsCaKGyUaYxWwtiVSDwD51sNgR-hskXiELYo1HN0kDqCiQMfowNH-5cqJqqjqiCy46KsOtK3FlK6c_zjh96yhIS4DhZGccpqT5Dssuols03iEEI_h8KvemSe7HEtfnwdWovwEUlsBkXtc7BRb6LIH2ZrekTcJDZLMJBB6Tcf3pVN9we22DLQYKuNqDc3TK962D17-bKlVCnSB2J_--VPrw"
      },
      "type": "object",
      "properties": {
        "auth_token": {
          "example": "eyJraWQiOiJNSytSKzRhYUk4YjBxVkhBMkZLZFN4Ykdpb3RXbTNXOGhZWE45dXF3K3YwPSIsImFsZyI6IlJTMjU2In0",
          "type": "string"
        }
      },
      "required": [
        "auth_token"
      ]
    },
    "AuthenticationErrorResponse": {
      "title": "AuthenticationErrorResponse",
      "description": "Error message for failed authentication",
      "type": "object",
      "properties": {
        "message": {
          "example": "Access denied - please verify whether supplied credentials are correct. Kindly note that credentials are case sensitive. If problem persists, please contact your API development team.",
          "type": "string"
        }
      },
      "required": [
        "message"
      ]
    },
    "ClientRequestErrorResponse": {
      "title": "ClientRequestErrorResponse",
      "description": "Response message for bad request - malformed client request syntax",
      "example": {
        "correlationId": "ddd65e40-0deb-11e9-9ae2-02b5324a36fc",
        "message": "Bad client request",
        "details": "Request could not be understood by the server due to malformed syntax. Please check amd modify the structure of your JSON and repeat your request."
      },
      "type": "object",
      "properties": {
        "correlationId": {
          "example": "458ae670-0dea-11e9-9ae2-02b5324a36fc",
          "type": "string"
        },
        "message": {
          "example": "Bad client request",
          "type": "string"
        },
        "details": {
          "example": "Request could not be understood by the server due to malformed syntax. Please check amd modify the structure of your JSON and repeat your request.",
          "type": "string"
        }
      },
      "required": [
        "correlationId",
        "message",
        "details"
      ]
    },
    "CountriesResponse": {
      "title": "CountriesResponse",
      "description": "List of countries where company reports are available",
      "type": "object",
      "properties": {
        "countryName": {
          "example": "Great Britain",
          "type": "string"
        },
        "countryISO": {
          "example": "GB",
          "type": "string"
        }
      }
    },
    "SearchParametersSet": {
      "title": "SearchParametersSet",
      "description": "Search parameters available for selected countries to refine search. When multiple countries are supplied will return search parameters that are common between those countries.",
      "type": "object",
      "properties": {
        "searchParameters": {
          "$ref": "#/definitions/SearchParameters"
        }
      },
      "required": [
        "searchParameters"
      ]
    },
    "SearchParameters": {
      "title": "SearchParameters",
      "type": "object",
      "properties": {
        "id": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "safeNo": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "regNo": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "vatNo": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "name": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "exact": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "street": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "houseNo": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "city": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "postCode": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "province": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "phone": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "officeType": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "status": {
          "$ref": "#/definitions/SearchParameterType"
        },
        "type": {
          "$ref": "#/definitions/SearchParameterType"
        }
      }
    },
    "SearchParameterType": {
      "title": "SearchParameterType",
      "description": "Search Parameter used to narrow down result in company search",
      "type": "object",
      "properties": {
        "required": {
          "type": "boolean"
        },
        "allowedValues": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "minLength": {
          "type": "integer",
          "format": "int32"
        },
        "maxLength": {
          "type": "integer",
          "format": "int32"
        },
        "regex": {
          "type": "string"
        }
      },
      "required": [
        "required"
      ]
    },
    "SearchParametersResponse": {
      "title": "SearchParametersResponse",
      "description": "Response message containing valid search parameters",
      "type": "object",
      "properties": {
        "countries": {
          "description": "List of countries",
          "type": "array",
          "items": {
            "$ref": "#/definitions/CountryCodes"
          }
        },
        "searchParametersSet": {
          "description": "Set of seach parameters applicable to the selected countries",
          "type": "array",
          "items": {
            "$ref": "#/definitions/SearchParametersSet"
          }
        }
      }
    },
    "CompaniesSearchResponse": {
      "title": "CompaniesSearchResponse",
      "description": "Paginated list companies matching provided search parameters",
      "example": {
        "value": {
          "id": "GB001-1-4257085",
          "country": "GB",
          "safeNo": "UK11538192",
          "name": "DHL",
          "address": {
            "line": "LINK 20 BUSINESS PARK, LARKFIELD, AYLESFORD, KENT, ME20 6SP",
            "street": "LARKFIELD",
            "city": "AYLESFORD",
            "postCode": "ME20 6SP"
          },
          "status": "Active",
          "type": "NonLtd",
          "dateOfLatestChange": "2011-09-27T23:00:00Z"
        }
      },
      "type": "object",
      "properties": {
        "pagination": {
          "$ref": "#/definitions/Paging"
        },
        "companies": {
          "description": "List of companies matching provided search parameters",
          "type": "array",
          "items": {
            "$ref": "#/definitions/CompanySearchResult"
          }
        }
      }
    },
    "Paging": {
      "title": "Paging",
      "description": "Information on API pagination",
      "type": "object",
      "properties": {
        "pageNumber": {
          "example": 1,
          "type": "integer",
          "format": "int32"
        },
        "itemsPerPage": {
          "example": 10,
          "type": "integer",
          "format": "int32"
        },
        "TotalSize": {
          "example": 3,
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "CompanySearchResult": {
      "title": "CompanySearchResult",
      "description": "Information on company matching provided search results",
      "type": "object",
      "properties": {
        "id": {
          "description": "Company identification number, used to order company report",
          "example": "GB001-1-425708",
          "type": "string"
        },
        "country": {
          "description": "Country where the company is registered",
          "example": "GB",
          "type": "string"
        },
        "safeNo": {
          "description": "API internal unique identification number",
          "example": "UK21538192",
          "type": "string"
        },
        "name": {
          "description": "Registered company name/trading name of the business.",
          "example": "DHL",
          "type": "string"
        },
        "address": {
          "$ref": "#/definitions/Address"
        },
        "status": {
          "$ref": "#/definitions/Status"
        },
        "type": {
          "$ref": "#/definitions/Type"
        },
        "dateOfLatestChange": {
          "description": "Date of last change on company profile",
          "example": "9/27/2011 11:00:00 PM",
          "type": "string"
        }
      },
      "required": [
        "id",
        "country",
        "safeNo",
        "name",
        "address",
        "status",
        "type",
        "dateOfLatestChange"
      ]
    },
    "Address": {
      "title": "Address",
      "description": "Address of the company",
      "type": "object",
      "properties": {
        "line": {
          "example": "LINK 20 BUSINESS PARK, LARKFIELD, AYLESFORD, KENT, ME20 6SP",
          "type": "string"
        },
        "street": {
          "example": "LARKFIELD",
          "type": "string"
        },
        "city": {
          "example": "AYLESFORD",
          "type": "string"
        },
        "postCode": {
          "example": "ME20 6SP",
          "type": "string"
        }
      }
    },
    "CompanyReportResponse": {
      "title": "CompanyReportResponse",
      "type": "object",
      "properties": {
        "orderId": {
          "example": "N/A",
          "type": "string"
        },
        "companyId": {
          "example": "GB-1-4257085",
          "type": "string"
        },
        "dateOfOrder": {
          "example": "1/3/2019 2:57:44 PM",
          "type": "string"
        },
        "language": {
          "example": "en",
          "type": "string"
        },
        "chargeRef": {
          "type": "string"
        },
        "report": {
          "$ref": "#/definitions/CompanyReport"
        }
